package com.bit.day23;

import java.awt.Button;
import java.awt.Frame;
import java.awt.Panel;

public class Ex10 extends Frame{
	public Ex10() {
		Panel p = new Panel();
		p.setLayout(null);
		
		Button btn = new Button();
		
		add(p);
		setSize(800, 600);
		setLocation(200,200);
		setVisible(true);
	}
	public static void main(String[] args) {
		new Ex10();
	}
}
