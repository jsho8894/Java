package src.com.bit.day11;

public class Ex02 {
    public static void main(String[] args) {
        //byte -> Byte
        //short -> Short
        //long -> Long
        byte su11 = 10;
        Byte su1= new Byte(su11);
        short su22 = 10; 
        Short su2 = new Short(su22);
        long su33 = 10L;
        Long su3 = new Long("10");
        //Long su3 = new Long(10L);//입력된 값이 int이므로 자동 형변환되기떄문에 오류가 나오지 않습니다.
        System.out.println(su3);
    }
}