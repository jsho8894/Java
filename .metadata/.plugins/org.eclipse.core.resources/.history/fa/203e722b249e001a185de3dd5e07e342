package com.bit.day14;
class Machine{
	void on(){
		System.out.println("환영합니다.");
	}
	void off(){
		System.out.println("전원을 종료합니다.");
	}
}

class Tv(){
	void on(){
		System.out.println("환영합니다.");
	}
	void off(){
		System.out.println("전원을 종료합니다.");
	}
	void work(){
		System.out.println("화면과 소리를 재상한다.");
	}
}

public class Ex02 {
	public static void main(String[] args) {
		
	}
}
