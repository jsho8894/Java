package com.bit.day14;

public class Ex15 {
	public static void main(String[] args) {
		int[] arr1 = {1,3,5,7};
		
		System.out.println(arr1);
		System.out.println(java.util.Arrays.toString(arr1));//배열의 내용을 확인
		
		int[] arr2 = new int[arr1.length];
		for(int i = 0; i < arr1.length; i++){
			arr2[i] = arr1[i];
		}
		
	}
}
