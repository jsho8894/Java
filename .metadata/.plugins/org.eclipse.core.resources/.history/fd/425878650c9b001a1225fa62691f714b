package src.com.bit.day11;
class Lec08{//부모클래스
    int su = 1234;
    void func01(){
        System.out.println("Lec08 func01...");
    }
}
public class Ex08 extends Lec08{//자식클래스, Ex클래스는 Lec08클래스에 상속을 받습니다
    public static void main(String[] args) {
         Lec08 you = new Lec08();
        // you.func01();
        //Ex08 me = new Ex08();
        System.out.println(you.su);
        //me.func01();

    }
    void T08(){
        System.out.println("T08");
    }
}