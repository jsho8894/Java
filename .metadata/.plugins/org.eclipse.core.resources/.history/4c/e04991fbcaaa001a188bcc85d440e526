package com.bit.day26;

public class Ex06 extends Thread{

	public void run(){
		
	}
	
	public static void main(String[] args) {
		//1~10000까지의 합계를 구하시오
		//1~5000까지의 합계를 구하시오
		//5001~10000까지의 합계를 구하시오
		int sum = 0;
		for(int i = 0; i<10000; i++)sum+=i+1;
		for(int i = 5000; i<10000; i++)sum+=i+1;
		
		System.out.println(sum);
	}
}
